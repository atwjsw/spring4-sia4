<ehcache>
    <defaultCache
            name="defaut"
            maxElementsInMemory="5"
            eternal="false"
            timeToIdleSeconds="20"
            timeToLiveSeconds="20"
            overflowToDisk="false"
            diskPersistent="false"
            memoryStoreEvictionPolicy="LRU"
    />
    <cache name="spittleCache" maxBytesLocalHeap="50m" timeToLiveSeconds="100">
    </cache>
</ehcache>